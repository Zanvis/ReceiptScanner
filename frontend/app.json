{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "scheme": "receiptscanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Pozwól aplikacji używać aparatu do skanowania paragonów"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Pozwól aplikacji zapisywać zdjęcia paragonów",
          "savePhotosPermission": "Pozwól aplikacji zapisywać zdjęcia paragonów"
        }
      ],
      "expo-dev-client"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.antoni.receiptscanner"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.antoni.receiptscanner",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
